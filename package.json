    "class-validator": "^0.14.0",
    "dayjs": "^1.11.7",
    "dinero.js": "^2.0.0-alpha.14",
    "lodash": "^4.17.21",
    "meteor-node-stubs": "^1.2.5",
    "neverthrow": "^6.0.0",
